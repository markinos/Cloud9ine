            $('#studentGenEdit').val(generation);
            $('#studentEmailEdit').val(email);
            $('#studentDegreeEdit').val(degree);
            $('#studentGradYearEdit').val(gradYr);
            $('#studentGradTermEdit').val(gradTrm);
            $('#studentDegreeEdit').val(degree);
            $('#studentGPAEdit').val(gpa);

            if (contact == "Yes") {
                $('#radio1Edit').prop("interdeterminate", true);

            }

            $("#editGradModal").modal('show');



        });


    });
