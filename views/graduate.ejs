    <script type="text/javascript">
    // for deleting a graduate from the system
    $(document).ready(function() {
        var table = $('#example1').DataTable();
        $('#example1 tbody').on('click', '.btn-danger', function() {

            var gradId = table.row($(this).parents('tr')).data();

            console.log("delete this id: " + gradId[1]);

            table
                .row($(this).parents('tr'))
                .remove()
                .draw();

            $.ajax({
                url: '/delete',
                data: {
                    gradId: gradId[1]
                },
                type: 'POST',
            });
        });

    });
    </script>
