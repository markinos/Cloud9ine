        $('#example1 tbody').on('click', '.btn-info', function() {
            console.log("edit grad");
            var gradInfo = table.row($(this).parents('tr')).data();

            var id = gradInfo[1];
            var lastName = gradInfo[2];
            var firstName = gradInfo[3];
