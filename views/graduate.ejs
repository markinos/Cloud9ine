            var gpa = gradInfo[13];
            var contact = gradInfo[14];


            console.log("grad info: " + firstName + " " + lastName + " " + id + " " + program + " " + contact);


            $('#studentIdEdit').val(id);
            $('#studentLastNameEdit').val(lastName);
            $('#studentFirstNameEdit').val(firstName);
